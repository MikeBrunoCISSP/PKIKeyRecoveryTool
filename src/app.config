<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>

    <!--Logging Verbosity-->
    <!-- Critical - Only critical errors are logged-->
    <!--   Errors - Only errors are logged-->
    <!--  Warning - Only errors and warnings are logged-->
    <!--     Info - Informational messages, warnings and errors are logged-->
    <!--  Verbose - Diagnostic logging-->
    <add key="LogLevel" value="Verbose" />


  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="BouncyCastle.Crypto" publicKeyToken="0e99375e54769942" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.8.10.0" newVersion="1.8.10.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
